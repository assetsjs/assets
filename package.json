{
  "name": "assets",
  "version": "3.0.1",
  "description": "An asset manager for node",
  "keywords": [
    "assets",
    "base64",
    "image",
    "path",
    "size",
    "url"
  ],
  "bugs": "https://github.com/borodean/assets/issues",
  "license": "MIT",
  "author": "Vadym Borodin <borodean@gmail.com> (http://borodean.com)",
  "main": "./lib",
  "repository": "borodean/assets",
  "scripts": {
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "eslint --ignore-path .gitignore . && nyc --reporter=text --reporter=html ./node_modules/ava/cli.js"
  },
  "dependencies": {
    "async": "^3.2.0",
    "bluebird": "^3.7.2",
    "calipers": "^2.0.1",
    "calipers-gif": "^2.0.0",
    "calipers-jpeg": "^2.0.0",
    "calipers-png": "^2.0.0",
    "calipers-svg": "^2.0.1",
    "calipers-webp": "^2.0.0",
    "glob": "^7.1.6",
    "lodash": "^4.17.20",
    "mime": "^2.4.7"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "sinon": "^9.2.3"
  },
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,json,md}": "prettier --write"
  }
}
